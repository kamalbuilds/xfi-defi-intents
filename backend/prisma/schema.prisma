generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Transaction {
    id          String  @id @default(cuid())
    metadata    Json
    fromAddress String
    txHash      String?
}

model StakingTransaction {
    id          String  @id @default(cuid())
    metadata    Json
    fromAddress String
    toAddress   String
    amount      String
    txHash      String?
}